// database
datasource db {
  provider = "sqlite"
  url      = "file:database.db"
}

// generator
generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 5
}

// data models
model Server {
  id              BigInt @id
  confess_channel BigInt @default(0)
  cnuy_channel    BigInt @default(0)
}

model User {
  id        BigInt @id
  sex_count BigInt @default(1)
}
